<p id="notice"><%= notice %></p>

<h1>Games</h1>

<table>
  <thead>
    <tr>
      <th>Home team</th>
      <th>Away team</th>
      <th>Winning team</th>
      <th>Losing team</th>
      <th>Winning score</th>
      <th>Losing score</th>
      <th>Week</th>
      <th>Year</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.home_team %></td>
        <td><%= game.away_team %></td>
        <td><%= game.winning_team %></td>
        <td><%= game.losing_team %></td>
        <td><%= game.winning_score %></td>
        <td><%= game.losing_score %></td>
        <td><%= game.week %></td>
        <td><%= game.year %></td>
        <td><%= link_to 'Show', game %></td>
        <td><%= link_to 'Edit', edit_game_path(game) %></td>
        <td><%= link_to 'Destroy', game, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game', new_game_path %>
